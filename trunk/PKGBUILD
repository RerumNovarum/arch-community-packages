pkgname=inxi
pkgver=1.7.32
pkgrel=1
pkgdesc="script to get system information"
arch=('any')
url="http://inxi.org"
license=('GPL')
source=(ftp://ftp.archlinux.org/other/community/$pkgname/$pkgname-$pkgver.gz)
md5sums=('06b491f0f07e72f0e755af7d9e8336b1')

package() {
  cd "$srcdir"
  install -D -m755 inxi-$pkgver "$pkgdir/usr/bin/inxi"
}

