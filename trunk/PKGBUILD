pkgname=wine_gecko
pkgver=0.9.1
pkgrel=1
pkgdesc="Wine's built-in replacement for Microsoft's Internet Explorer"
arch=('i686')
url="http://www.winehq.org/"
license=('MPL')
depends=('wine')
source=(http://downloads.sourceforge.net/sourceforge/wine/${pkgname}-${pkgver}.cab)
md5sums=('6abd26c3caf66b497acf73958b5119a5')

build() {
    install -Dm644 ${srcdir}/${pkgname}-${pkgver}.cab \
      ${pkgdir}/usr/share/wine/gecko/${pkgname}-${pkgver}.cab
}
