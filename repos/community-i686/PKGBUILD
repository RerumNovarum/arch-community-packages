pkgname=python-cssutils
pkgver=0.9.6b1
pkgrel=2
pkgdesc="A CSS Cascading Style Sheets library for Python"
arch=('i686' 'x86_64')
url="http://code.google.com/p/cssutils/"
license=('LGPL3')
conflicts=('python-cssutils-legacy')
depends=('python')
makedepends=('unzip' 'setuptools')
source=(http://cssutils.googlecode.com/files/cssutils-$pkgver.zip)
md5sums=('1ef2b7e7898ca1d778703e34f8f3a46b')

build() {
  cd "$srcdir/cssutils-$pkgver"
  python setup.py install --root="$pkgdir" || return 1
}
